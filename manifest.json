{

"name": "Anime-sion",

"description": "Build an Extension!",

"version": "1.0.0",

"manifest_version": 3,

"web_accessible_resources":[
  {
    "resources":["images/*.jpg"],
    "matches":["<all_urls>"]
  }

],

"content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],




"permissions":["storage","activeTab", "scripting"]


}
